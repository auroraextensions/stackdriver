<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <stackdriver>
            <general>
                <enable>0</enable>
            </general>
            <logging>
                <log_channel>magento</log_channel>
                <include_log_context>0</include_log_context>
                <log_levels>emergency,alert,critical,error</log_levels>
            </logging>
        </stackdriver>
    </default>
</config>
